    "import numpy as np\n",
    "import matplotlib.pyplot as pt\n",
    "\n",
    "x = np.arange(0,360)\n",
    "y = np.sin( x * np.pi / 180.0)\n",
    "\n",
    "pt.plot(x,y)\n",
    "pt.xlim(0,360)\n",
    "pt.ylim(-1.2,1.2)\n",
    "pt.title(\"SIN function demo\")\n",
    "pt.show()"
   ]
